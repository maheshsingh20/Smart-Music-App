NODE_ENV=development
PORT=5000
DATABASE_URL=mongodb://localhost:27017/musicdb
REDIS_URL=redis://localhost:6379
JWT_SECRET=your-secret-key-change-in-production
JWT_REFRESH_SECRET=your-refresh-secret-change-in-production
STRIPE_SECRET_KEY=sk_test_your_stripe_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# Music Provider: 'jamendo' (full songs!), 'spotify' (30s previews), or 'dummy' (mock data)
MUSIC_PROVIDER=jamendo

# Jamendo API - FREE with full song playback! (Get from: https://devportal.jamendo.com/)
JAMENDO_CLIENT_ID=56d30c95

# Spotify API Credentials (Get from: https://developer.spotify.com/dashboard)
SPOTIFY_CLIENT_ID=your_spotify_client_id
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret

CORS_ORIGIN=http://localhost:5173
